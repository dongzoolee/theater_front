                          </tr>\n                                <tr>\n                                    {this.state.content.map((val, idx) => {\n                                        return (\n                                            <th className={styles.StoryLineStoryTh} key={idx}>\n                                                <StoryPreview\n                                                    idx={idx}\n                                                    storyId={val.idx}\n                                                    title={val.title}\n                                                    date={val.date}\n                                                    content={val.content}\n                                                />\n                                            </th>\n                                        )\n                                    })}\n                                </tr>\n                            </tbody>\n                        </table>\n                    </div>\n                </div>\n                <div className={styles.adfit} />\n            </>\n        )\n    }\n}\nexport default withRouter(StoryLine);\n","/home/ubuntu/docker/html/blog/react/src/components/StoryLine/MobileContentWrapper/MobileContentWrapper.js",["126"],"import React, { Component } from 'react';\nimport { Link } from 'react-router-dom';\nimport styles from './MobileContentWrapper.module.scss';\nimport axios from 'axios';\nimport Slider from \"react-slick\";\nimport \"slick-carousel/slick/slick.css\";\nimport \"slick-carousel/slick/slick-theme.css\";\nimport StoryPreview from '../../StoryPreview/StoryPreview';\n\nclass MobileContentWrapper extends Component {\n    state = {\n    }\n    getTextElement = (s) => {\n        let span = document.createElement('span');\n        span.innerHTML = s;\n        return span.textContent || span.innerText;\n    }\n    getImgSrc = (s) => {\n        let regex = /<img.*?src=\"([^\">]*\\/([^\">]*?))\".*?>/g, ret;\n        ret = regex.exec(s);\n        if (ret)\n            return ret[1];\n        else\n            return null;\n    }\n    componentDidMount() {\n    }\n    render() {\n        const settings = {\n            dots: true,\n            infinite: true,\n            slidesToShow: 1,\n            slidesToScroll: 1,\n            autoplay: true,\n            speed: 1300, // animation speed\n            // autoplaySpeed: 2000,\n            cssEase: \"linear\"\n        };\n        return (\n            <>\n                <div className={styles.Header}>\n                    <div>스토리라인</div>\n                </div>\n                <div className={styles.StorySlideWrapper}>\n                    <div className={styles.StorySlide}>\n                        <Slider {...settings}>\n                            {this.props.content.map((val, idx) => {\n                                return (\n                                    <>\n                                        <div className={styles.StorySlideHeader}><Link to={'/' + val.mainCategory}>{val.mainCategory}</Link></div>\n                                        <StoryPreview\n                                            idx={0}\n                                            storyId={val.idx}\n                                            title={val.title}\n                                            date={val.date}\n                                            content={val.content}\n                                        />\n                                    </>\n                                )\n                            })}\n                        </Slider>\n                    </div>\n                </div>\n                <div className={styles.StoryLineWrapper}>\n                    <div className={styles.StoryLineContentWrapper + \" non--draggable\"}>\n                        <Link to=\"/보물창고\">\n                            <div className={styles.BigStoryLine}>\n                                <div className=